{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", ".contents/data.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"],
			"@/scripts/*": ["./scripts/*"],
			"@/contents/*": ["./.contents/*"]
		},
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"types": ["@types/node", "./worker-configuration.d.ts"]
	}
}
